{
	"name": "mensa-crawler",
	"version": "2.1.2",
	"private": true,
	"description": "Crawls the canteen data of student canteens in Hamburg",
	"license": "MIT",
	"repository": "HAWHHCalendarBot/mensa-crawler",
	"author": {
		"name": "EdJoPaTo",
		"email": "edjopato@gmail.com",
		"url": "https://edjopato.de"
	},
	"engines": {
		"node": ">=12"
	},
	"scripts": {
		"build": "del dist && tsc",
		"prepack": "npm run build",
		"start": "ts-node source/index.ts",
		"test": "tsc --sourceMap && xo && ava"
	},
	"main": "dist/source",
	"types": "dist/source",
	"files": [
		"dist/source",
		"!*.test.*"
	],
	"dependencies": {
		"array-reduce-group-by": "^1.0.0",
		"got": "^11.0.1",
		"json-stable-stringify": "^1.0.1"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.7.0",
		"@types/json-stable-stringify": "^1.0.32",
		"@types/node": "^13.1.4",
		"ava": "^3.7.1",
		"del-cli": "^3.0.0",
		"ts-node": "^8.5.4",
		"typescript": "^3.7.4",
		"xo": "^0.29.1"
	},
	"xo": {
		"semicolon": false,
		"rules": {
			"@typescript-eslint/prefer-readonly-parameter-types": "warn",
			"ava/no-ignored-test-files": "off"
		}
	}
}
